  <SEGdef id="SigTail" 
          code="HNSHA" 
          version="2" 
          pversion="300"
          delimiter="+" 
          terminator="'"
          mode="RDH"
  >
    <!-- "Signaturabschluss" -->
    <GROUP type="SegHead" name="head" />
    <ELEM name="ctrlref" type="ascii" maxsize="14" />
    <ELEM name="signature" type="bin" maxsize="512" />
 
    <VALUES>
      <VALUE path="head/code">%code</VALUE>
