      ab_userqueue.tm2 ab_userqueue_list.tm2
      ab_swift_descr.tm2 ab_swift_descr_list.tm2
    </data>


    <extradist>
      provider_account.c
      provider_accspec.c
      provider_user.c
      provider_queues.c
      provider_utils.c
    </extradist>


    <useTargets>
    </useTargets>

  
  

    <!-- changes below probably not needed except <subdirs> -->



    <!-- don't install, don't distribute -->
    <headers dist="false" >
      $(local/built_headers_pub)
      $(local/built_headers_priv)
    </headers>


    <!-- don't install but distribute -->
    <headers dist="true" >
      $(local/headers_priv)
      $(local/headers_pub)
    </headers>


    <sources>
      $(local/typefiles)
      $(local/sources)
    </sources>




    <subdirs>
