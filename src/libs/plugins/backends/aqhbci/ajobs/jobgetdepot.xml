</SEGs>


<JOBs>

 <JOBdef 
    id="JobGetDepot" 
    code="HKWPD" params="HIWPDS"
    version="5"
    crypt="1" 
    sign="1" 
    needBPD="1"
    response="DepotResponse"
    attachable="1"
    single="1"
 >
   <SEG type="GetDepot" version="5" />

   <VALUES>
     <VALUE path="head/code">%code</VALUE>
     <VALUE path="head/version">%version</VALUE>
     <VALUE path="allAccounts">N</VALUE>
   </VALUES>
 </JOBdef>



